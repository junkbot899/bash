#!/usr/bin/env bash

grr () {

	echo "tir.tbx[Texas Instruments Repo]"

	if [ -z "${1}" ]; then
		echo "\${1}: <comment> (git commit): Not provided!"
		exit
	fi


	if [ -z "${2}" ]; then
		echo "\${2}: <branch-name>: Not provided!"
		exit
	fi

	cp -r ../../custom_libraries ./custom_libraries/
	git add .
	git commit -am "${1}"
	git push origin "${2}"
}

grr "${1}" "${2}" 
